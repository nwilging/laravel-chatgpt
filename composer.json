{
    "name": "nwilging/laravel-chatgpt",
    "description": "Super simple wrapper for openai-php/client with error handling. Specifically for ChatGPT conversations.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nwilging\\LaravelChatGpt\\": "src/",
            "Nwilging\\LaravelChatGptTests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nick Wilging",
            "email": "nick@midwestdrifting.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=8",
        "guzzlehttp/guzzle": "^7.4",
        "openai-php/client": "^0.6.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nwilging\\LaravelChatGpt\\Providers\\ChatGptServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "*",
        "php-mock/php-mock": "^2.4",
        "dg/bypass-finals": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
